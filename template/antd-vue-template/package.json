{
  "name": "{{ name }}",
  "version": "{{ version }}",
  "private": true,
  "scripts": {
    "serve": "cross-env NODE_ENV=development vue-cli-service serve",
    "serve:mock": "npm run serve -- --mode mock",
    "serve:dev": "npm run serve -- --mode dev",
    "serve:alpha": "npm run serve -- --mode alpha",
    "serve:rc": "npm run serve -- --mode rc",
    "serve:prod": "npm run serve -- --mode prod",
    "build": "cross-env NODE_ENV=production vue-cli-service build",
    "build:dev": "npm run build -- --mode dev",
    "build:alpha": "npm run build -- --mode alpha",
    "build:rc": "npm run build -- --mode rc",
    "build:prod": "npm run build -- --mode prod",
    "prepare": "husky install",
    "lint:js": "eslint --ext .js,.ts,.tsx ./src",
    "lint:css": "stylelint ./src/**/*.{css,less}",
    "lint": "npm run lint:js && npm run lint:css",
    "format:js": "eslint --fix --ext .js,.ts,.tsx ./src",
    "format:css": "stylelint --fix ./src/**/*.{css,less}",
    "format": "npm run format:js && npm run format:css",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{vue,ts,tsx,js,jsx}": "eslint --fix",
    "*.{vue,css,less,sass,scss}": "stylelint --fix"
  },
  "dependencies": {
    "@ztjy/antd-vue": "^1.0.0-beta.51",
    "ant-design-vue": "~2.1.6",
    "core-js": "^3.6.5",
    "vue": "^3.0.0",
    "vue-class-component": "^8.0.0-0",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@ant-design-vue/vue-jsx-hot-loader": "^0.1.4",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@ztjy/eslint-config-vue": "^1.1.0",
    "@ztjy/prettier-config": "^1.0.9",
    "@ztjy/stylelint-config": "^1.0.0",
    "babel-plugin-import": "^1.13.3",
    "cross-env": "^7.0.3",
    "eslint": "^7.0.0",
    "husky": "^7.0.1",
    "less": "^4.1.1",
    "less-loader": "^7.3.0",
    "lint-staged": "^11.0.0",
    "mocker-api": "^2.9.0",
    "mockjs": "^1.1.0",
    "prettier": "^2.0.0",
    "stylelint": "^13.0.0",
    "typescript": "~4.1.5"
  }
}
